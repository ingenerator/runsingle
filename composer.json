{
    "name": "ingenerator/runsingle",
    "description": "Wrapper package for ensuring a script is only run once across multiple instances.",
    "homepage": "https://github.com/ingenerator/runsingle",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "inGenerator",
            "homepage": "http://www.ingenerator.com/"
        },
        {
            "name": "Matthias Gisder",
            "email": "matthias@ingenerator.com",
            "homepage": "http://www.ingenerator.com"
        }
    ],
    "require": {
        "php":     ">=5.5",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.0",
        "phpspec/prophecy": "^1.1",
        "bossa/phpspec2-expect": "^2.0"
    },
    "suggest": {
    },
    "support": {
        "issues": "https://github.com/ingenerator/runsingle/issues",
        "source": "https://github.com/ingenerator/runsingle"
    },
    "autoload": {
        "psr-0": {
            "Ingenerator": "src/",
            "spec": ""
        }
    },
    "bin": ["bin/run_single.php"],
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
